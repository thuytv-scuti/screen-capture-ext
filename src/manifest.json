{
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "Eduα Snapshot",
  "description": "Create snapshot of current tab and insert to Eduα Cloud",
  "background": { "service_worker": "background.bundle.js" },
  "action": {},
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png", "popup.html"],
      "matches": [ "*://*/*" ]
    }
  ],
  "permissions": ["tabs", "scripting", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*", "<all_urls>", ""]
}
